    "react-css-modules": "^4.7.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-transition": "0.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
